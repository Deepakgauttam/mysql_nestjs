// Try Prisma Accelerate: 

datasource db {
  provider = "mysql" // Or your chosen database (postgresql, sqlite, etc.)
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id     Int     @id @default(autoincrement())
  name   String
  number String
  email  String @unique
}
